{
	"manifest_version": 2,
	"name": "Docs Comments Drawer",
	"short_name": "DocsCommentsDrawer",
	"description": "Transforms the Google Docs comments/suggestions popout into a useful drawer!",
	"version": "1.0.0",
	"content_scripts": [{
		"matches": ["https://docs.google.com/document/d/*"],
		"js": ["index.js", "index.css.js"]
	}],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}